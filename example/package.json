{
  "name": "jest-runner-minitest-example",
  "version": "0.1.0",
  "scripts": {
    "test": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "rb"
    ],
    "runner": "<rootDir>/../build",
    "testMatch": [
      "**/?(*_)test.rb"
    ]
  },
  "devDependencies": {
    "jest": "^25.4.0"
  }
}
